{
	"parserOptions": {
		"ecmaVersion": 2020
	},
	"parser": "@typescript-eslint/parser",
	"extends": ["standard", "eslint:recommended", "plugin:jest/recommended", "plugin:@typescript-eslint/recommended"],
	"env": {
		"node": true,
		"jest/globals": true
	},
	"rules": {
		"camelcase": 0,
		"indent": 0,
		"no-tabs": 0,
		"@typescript-eslint/no-explicit-any": 1
	},
	"settings": {
		"jest": {
			"version": 26
		}
	}
}
